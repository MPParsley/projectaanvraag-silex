<p class="cnw_searchresult-detail-block-title">{{ settings.articles.label }}</p>

<div>
  <ul class="cnw_searchresult-detail-articles cnw_col-12">
    {% for article in articles %}
      <li>
        <a class="cnw_searchresult-detail-article-title" href="{{ article.url }}" target="_blank">
          <strong>{{ article.headline }}</strong>
        </a>
        <p class="cnw_searchresult-detail-article-text">
          {{ article.text }}
          (bron: {{ article.publisher }})
        </p>
      </li>
    {% endfor %}
  </ul>
</div>



